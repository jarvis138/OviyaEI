{
  "title": "ASR Voice Pipeline",
  "description": "Silero VAD + Whisper ASR pipeline for real-time speech recognition",
  "type": "serverless",
  "category": "audio",
  "iconUrl": "https://raw.githubusercontent.com/snakers4/silero-vad/main/assets/icon.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 50,
    "presets": [
      {
        "name": "Default",
        "defaults": {
          "NO_TORCH_COMPILE": "1"
        }
      }
    ],
    "env": [
      {
        "key": "NO_TORCH_COMPILE",
        "value": "1"
      }
    ]
  }
}


